<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.nj.storm.service.sso.mapper.DemoMapper">
    <resultMap id="BaseResultMap" type="cn.nj.storm.service.sso.bean.DemoBean">
        <id column="id" jdbcType="INTEGER" property="id" />
        <result column="name" jdbcType="VARCHAR" property="name" />
        <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
    </resultMap>
    <select id="selectDemoAll" resultMap="BaseResultMap">
        select id,name,create_time from demo
    </select>
</mapper>
